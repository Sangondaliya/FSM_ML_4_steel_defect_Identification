with open ("finaltrain.csv","r") as ftr:
    xfr=ftr.read()
    p=xfr.split("\n")
    t=0
with open (r"C:\other\FSM\level_3_ml\code\finaltrain1.csv","w") as f1:
        
    for i in range(len(p)):
        t+=1
        r=[]
        q=p[i].split(",")
        for j in range(len(q)):
            if j%2==0:
                x=int(q[j])
                y=int(q[j+1])
                #print("x",x,"y",y)
            sum=[]
            num=x
            while(num<=x+y):
                #num=num+","
                sum.append(num)
                num+=1
            r+=sum
        f1.write(str(r))   
    f1.write("\n")                 
