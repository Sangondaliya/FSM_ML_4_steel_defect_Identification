import csv
import numpy as np 

b,c=[],[]
with open("train.csv","r") as t:
    x=t.read()
    #print(x)
    x=x.replace("\n",",")
    a=(x.split(","))

    for i in range(len(a)):
        if i%3==1:
            b.append(a[i])
        elif i%3==2:
            c.append(a[i])
